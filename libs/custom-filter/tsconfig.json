{
  "compilerOptions": {
    "target": "ES5",
    "outDir": "dist",
    "declaration": true,
    "module": "ESNext",
    "verbatimModuleSyntax": true,
    "moduleResolution": "node",
    "noUncheckedIndexedAccess": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true
  }
}
